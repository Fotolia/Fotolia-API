<settings xmlns="http://maven.apache.org/SETTINGS/1.1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd">

    <profiles>
        <profile>
            <id>fotolia-api-development</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <fotolia.api.key>your_registered_fotolia_api_key</fotolia.api.key>
                <fotolia.api.loginName>your_fotolia_login_name</fotolia.api.loginName>
                <fotolia.api.password>your_fotolia_password</fotolia.api.password>
            </properties>
        </profile>
    </profiles>

</settings>